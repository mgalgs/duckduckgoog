{
    "manifest_version": 2,
    "name": "DuckDuckGoog",
    "version": "1.0",
    "description": "Puts g! on a keyboard shortcut for quicker DDG ripcord pulling",
    "icons": {
        "48": "icons/icon-48.png"
    },
    "content_scripts": [
        {
            "matches": ["*://duckduckgo.com/*"],
            "js": ["duckduckgoog.js"]
        }
    ],
    "commands": {
        "pull-ripcord": {
            "suggested_key": {
                "default": "Ctrl+Comma"
            },
            "description": "Send current search to Google (via g!)"
        }
    }
}
